<h1>Users</h1>

<%users.forEach(user => { %>
    <li>
      <a href="/users/<%= user.id %>/"
        ><%= user.name %></a>
        <form action="/users/<%= user.id %>/?_method=DELETE" method="POST" >
      <button type="submit" class="btn btn-danger btn-sm">Delete</button>
    </form>
      
    </li>
    <% }) %>
  </ul> 